{
  "expo": {
    "name": "Open Talk",
    "slug": "opentalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.opentalk.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6366f1"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK",
        "VIBRATE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.opentalk.app",
      "versionCode": 1
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Open Talk to access your camera for video calls.",
          "microphonePermission": "Allow Open Talk to access your microphone for audio calls."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Open Talk to access your microphone for voice calls."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.24",
            "usesCleartextTraffic": true,
            "kotlinJvmTargetVersion": "17"
          },
          "ios": {}
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "827efb92-4ba9-4e58-bc77-50a95c7a42cd"
      }
    }
  }
}
